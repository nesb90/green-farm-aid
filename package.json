{
"name": "green-farm-aid",
  "version": "1.0.0",
  "description": "Utility for managing harvest",
  "main": "app.js",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/cors": "^10.0.1",
    "fastify": "^5.1.0",
    "fastify-jwt": "^4.2.0",
    "fastify-plugin": "^5.0.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.13.1"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "debug": "nodemon --inspect-brk=0.0.0.0:9229 app.js",
    "init:db": "node 'src/db/scripts/create-schema-db.js'",
    "db:migrate": "npx knex migrate:latest",
    "db:migrate:revert": "npx knex migrate:rollback",
    "pretest": "yarn init:db && yarn db:migrate",
    "test": "jest --reporters=default --detectOpenHandles",
    "test:debug": "nodemon --inspect-brk=0.0.0.0:9229 node_modules/jest/bin/jest.js --runInBand",
    "test:unit": "jest ./tests/**/*.test.js",
    "test:unit:coverage": "yarn test:unit --coverage"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "jest": "^29.7.0",
    "jest-silent-reporter": "^0.6.0"
  }
}
